<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Icon Aktif -->
    <item android:state_checked="true">
        <color android:color="#00000000" />
        <!-- Ganti @color/active_icon_color dengan warna yang kamu inginkan untuk icon aktif -->
    </item>
    <!-- Icon Tidak Aktif -->
    <item android:state_checked="false">
        <color android:color="#00000000" />
        <!-- Ganti @color/inactive_icon_color dengan warna yang kamu inginkan untuk icon tidak aktif -->
    </item>
</selector>
